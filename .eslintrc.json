{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console":"off",
        "no-undef":"off",
        "func-names":"off",
        "consistent-return":"off",
        "no-underscore-dangle":"off"
    }
}
